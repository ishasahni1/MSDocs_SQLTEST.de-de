    <Member MemberName="TargetEngineVersion">
      <MemberSignature Language="C#" Value="public Microsoft.SqlServer.Dac.EngineVersion TargetEngineVersion { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype Microsoft.SqlServer.Dac.EngineVersion TargetEngineVersion" />
