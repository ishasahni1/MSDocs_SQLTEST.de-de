{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
		  "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "api/overwrites/**",
          "_themes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api/overwrites/**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "externalReference": [],
    "globalMetadata": {
      "apiPlatform": "dotnet"
    },
    "fileMetadata": {
      "author": {
        "api/**/*.yml": "dotnet-bot"
      }
    },
    "template": [],
    "lruSize": 0,
    "dest": "sql-dotnet-api"
  }
}
